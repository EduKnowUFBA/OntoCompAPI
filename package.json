{
    "name": "ontoclass",
    "description": "Ontologia para o Classroom",
    "version": "0.0.1",
    "homepage": "http://chrome.google.com/webstore",
    "author": "Gabriel Tourinho Badar√≥ ",
    "repository": {
        "type": "git",
        "url": ""
    },
    "scripts": {
        "start": "xt-build -e dev -w",
        "start:firefox": "xt-build -e dev -p firefox -w",
        "build": "xt-build -e prod",
        "build:firefox": "xt-build -e prod -p firefox",
        "clean": "xt-clean",
        "docs": "xt-docs",
        "test": "xt-test",
        "coverage": "nyc --reporter=lcov npm run test",
        "sync": "xt-sync"
    },
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    },
    "eslintIgnore": [
        "test/**/*"
    ],
    "devDependencies": {
        "extension-cli": "latest"
    },
    "xtdocs": {
        "source": {
            "include": [
                "README.md",
                "src"
            ]
        }
    },
    "js": "./src/**/*.js",
    "xtbuild": {
        "js_bundles": [
            {
                "name": "background",
                "src": "./src/background.js"
            },
            {
                "name": "popup",
                "src": "./src/popup.js"
            },
            {
                "name": "competencies",
                "src": "./src/competencies.js"
            },
            {
                "name": "students",
                "src": "./src/students.js"
            },
            {
                "name": "course",
                "src": "./src/course.js"
            },
            {
                "name": "addcompetence",
                "src": "./src/addcompetence.js"
            },
            {
                "name": "editcompetence",
                "src": "./src/editcompetence.js"
            },
            {
                "name": "compfunctions",
                "src": "./src/compfunctions.js"
            },
            {
                "name": "studentview",
                "src": "./src/studentview.js"
            },
            {
                "name": "graph",
                "src": "./src/graph.js"
            },
            {
                "name": "studentchoice",
                "src": "./src/studentchoice.js"
            },
            {
                "name": "studentgraph",
                "src": "./src/studentgraph.js"
            }
        ]
    },
    "dependencies": {
        "@frogcat/ttl2jsonld": "^0.0.8",
        "buffer": "^6.0.3",
        "d3": "^3.5.17",
        "n3": "^1.16.1",
        "sigma": "^2.3.0",
        "tree-model": "^1.0.7",
        "url": "^0.11.0"
    }
}
